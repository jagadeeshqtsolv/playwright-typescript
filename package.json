{
  "name": "typescript",
  "version": "1.0.0",
  "description": "Playwright automation with Type script demo project",
  "main": "index.js",
  "scripts": {
    "regression": "HEADLESS=false npx playwright test",
    "web": "HEADLESS=true npx playwright test --grep @Web",
    "api": "HEADLESS=true npx playwright test --grep @API",
    "firefox": "BROWSER_NAME=firefox HEADLESS=true NODE_ENV=test npx playwright test",
    "html-report": "HEADLESS=true npx playwright test --reporter=html && npx playwright show-report",
    "allureclean": "rm -rf allure-report",
    "allureexecution": "HEADLESS=true npx playwright test --reporter=html,line,allure-playwright",
    "alluregenerate": "allure generate ./allure-results --clean",
    "allureopen": "allure open ./allure-report"
  },
  "keywords": [],
  "author": "Jagadeesh Madhura",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@types/node": "^20.14.0",
    "@types/node-fetch": "^2.6.11",
    "allure-playwright": "^2.15.1",
    "exceljs": "^4.4.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "typescript": "^5.4.5"
  }
}
